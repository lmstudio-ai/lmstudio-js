  "compilerOptions": {
    "strict": true,
    "target": "ES2021",
    "moduleResolution": "node",
    "incremental": true,
    "noImplicitOverride": true,
    "sourceMap": true,
    "skipLibCheck": true,
    "stripInternal": true,
    "composite": true,
    "esModuleInterop": true
