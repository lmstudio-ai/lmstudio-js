{
  "name": "@lmstudio/cli",
  "version": "0.0.28",
  "description": "LM Studio CLI",
  "main": "dist/index.js",
  "scripts": {
    "build-rollup": "rollup -c",
    "build-inject-variables": "node ./injectVariables.js",
    "build": "npm run build-rollup && npm run build-inject-variables",
    "make-bin:win32": "powershell -File make-bin-win32.ps1",
